<odoo>
    <record id="ir_cron_bigquery_export" model="ir.cron">
        <field name="name">Export Data to BigQuery</field>
        <field name="model_id" ref="bigquery_techfinna.model_bigquery_techfinna_export"/>
        <field name="state">code</field>
        <field name="code">model.export_data_to_bigquery()</field>
        <field name="active" eval="True"/>
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="user_id" ref="base.user_root"/>
    </record>
</odoo>
